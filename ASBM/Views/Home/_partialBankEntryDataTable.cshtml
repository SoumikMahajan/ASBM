@model List<ASBM.Models.BankModel>

<table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th>SL</th>
            <th>A/C No</th>
            <th>A/C Name</th>
            <th>Bank Name</th>
            <th>Ifsc</th>
            <th>Fund Scheme Name</th>
        </tr>
    </thead>
    <tbody>
        @{ int rows = 1;}
        @if (Model != null && Model.Count > 0)
        {
            foreach (var obj in Model)
            {
                <tr class="inAc" style="color: black;font-size: 13px !important;">
                    <td>@rows</td>
                    <td>@obj.bank_account_no</td>
                    <td>@obj.bank_account_name</td>
                    <td>@obj.bank_name</td>
                    <td>@obj.bank_ifsc</td>
                    <td>@obj.fund_scheme_name</td>
                </tr>
                rows++;
            }
        }
    </tbody>
</table>


