@model List<ASBM.Models.BillSubmission>

<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th>SL</th>
            <th>Company Category</th>          
            <th>Company Name</th>          
            <th>Department</th>          
            <th>Pan No</th>          
            <th>GST No</th>          
            <th>Fund Name</th>          
            <th>Description of Work</th>          
            <th>BIll Amount</th>          
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var obj in Model)
            {
                <tr class="inAc" style="color: black;font-size: 13px !important;">
                    <td>@obj.bill_details_id_pk</td>
                    <td>@obj.bill_category_id_fk</td>
                    <td>@obj.bill_company_name</td>
                    <td>@obj.department_name</td>
                    <td>@obj.bill_pan</td>
                    <td>@obj.bill_gst</td>
                    <td>@obj.fund_scheme_name</td>
                    <td>@obj.bill_description</td>
                    <td>@obj.bill_amount</td>
                </tr>
            }
        }
    </tbody>
</table>


