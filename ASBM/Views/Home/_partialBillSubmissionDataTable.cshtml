@model List<ASBM.Models.BillSubmission>

<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th>Header</th>
            <th>Sub Header</th>          
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var obj in Model)
            {
                <tr class="inAc" style="color: black;font-size: 13px !important;">
                    <td>@obj.bill_details_id_pk</td>
                    <td>@obj.bill_CreateDate</td>
                    

                             
                </tr>
            }
        }
    </tbody>
</table>


