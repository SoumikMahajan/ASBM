@model ASBM.Models.MultipleModel

<table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th scope="col">SL</th>
            <th scope="col">DOCKET NO</th>
            <th scope="col">DATE</th>
            <th scope="col">STATUS</th>
        </tr>
    </thead>
    <tbody class="tbody">
    </tbody>
    <tbody>

            @{ int rows = 1;}
            @if (Model != null && Model.billingStatusList.Count > 0)
            {
                foreach (var obj in Model.billingStatusList)
                {
                    <tr class="inAc" style="color: black;font-size: 13px !important;">
                        <td>@rows</td>
                        <td>@obj.bill_allotement_docket_no</td>
                        <td>@obj.bill_allotement_date</td>
                        <td></td>
                    </tr>
                    rows++;
                }
            }
        </tbody>
</table>
