@model List<ASBM.Models.PayeeModel>

<table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th>ID</th>
            <th>Payee Name</th>
            <th>Mobile No</th>
            <th>Pan No</th>
            <th>GST NO</th>
            <th>A/C No</th>
            <th>Department Name</th>
        </tr>
    </thead>
    <tbody>
        @{ int rows = 1;}
        @if (Model != null && Model.Count > 0)
        {
            foreach (var obj in Model)
            {
                <tr class="inAc" style="color: black;font-size: 13px !important;">
                    <td>@rows</td>
                    <td>@obj.payee_name</td>
                    <td>@obj.payee_mobile</td>
                    <td>@obj.payee_pan</td>
                    <td>@obj.payee_gst</td>
                    <td>@obj.payee_account_no</td>
                    <td>@obj.department_name</td>
                </tr>
                rows++;
            }
        }
    </tbody>
</table>


