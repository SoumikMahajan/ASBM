@model List<ASBM.Models.RandomBillGenerationModel>

<table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr class="dataHead">
            <th>SL</th>
            <th>Bill Name</th>
            <th>Bill Type</th>
            <th>Department Name</th>
            <th>Fund Scheme Name</th>
            <th>Work Description</th>
            <th>Mobile No</th>
        </tr>
    </thead>
    <tbody>
        @{ int rows = 1;}
        @if (Model != null && Model.Count > 0)
        {
            foreach (var obj in Model)
            {
                <tr class="inAc" style="color: black;font-size: 13px !important;">
                    <td>@rows</td>
                    <td>@obj.random_bill_name</td>
                    <td>@obj.typeof_bill_name</td>
                    <td>@obj.department_name</td>
                    <td>@obj.fund_scheme_name</td>
                    <td>@obj.random_bill_work_desc</td>
                    <td>@obj.random_bill_mobile_no</td>
                </tr>
                rows++;
            }
        }
    </tbody>
</table>


