@model ASBM.Models.MultipleModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="container" style="background-color: white; color: black; padding-bottom: 20px;">
        <div class="row no-gutters">
            <div class="flex-column justify-content-center about-content" style="width: 100% !important;">
                <div class="section-title">
                    <h2>Accountant</h2>
                </div>
            </div>
        </div>

        <div class="container" style="background-color: beige;">

            <div id="prtCmsDataTable">
                @Html.Partial("~/Views/Accountant/_partialAccountantVouterDataTable.cshtml", Model)
            </div>
        </div>

        
        <form id="voucher" method="post" style="display:block;">
            <hr />
            <div class="container" style="background-color: beige;" id="VouterDetailsShowHide">
                <div class="row form-group">
                    <div class="col-md-3"><label>Voucher No : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtVouterNo" placeholder="" name="txtVouterNo" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                    <div class="col-md-3"><label>Voucher Date : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtVouterDate" placeholder="" name="txtVouterDate" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><h5><b>Voucher Details : </b></h5></div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Category of Company : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtComapanyCategory" placeholder="" name="txtComapanyCategory" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                    <div class="col-md-3"><label>Type of Work : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtWorkType" placeholder="" name="txtWorkType" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Comapany Name : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtComapanyName" placeholder="" name="txtComapanyName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                    <div class="col-md-3"><label>Department Name : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtDeptName" placeholder="" name="txtDeptName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Proprietor Name : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                    <div class="col-md-3"><label>Fund Name : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Mobile No : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                    <div class="col-md-3"><label>GST No : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtGst" placeholder="" name="txtGst" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Description of Work : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtDescWork" placeholder="" name="txtDescWork" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="form-group row justify-content-center text-center">
                    <div class="col-sm-12">
                        <button type="button" class="btn btn-info" id="btn_proceed_payment">Proceed for Payment</button>
                    </div>
                </div>
            </div>

            <hr />



            <div class="container" style="background-color: beige;" id="block_payment_calculation">
                <div class="row form-group">
                    <div class="col-md-3"><label>Basic Amount of Bill : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtBasicBillAmount" placeholder="" name="txtBasicBillAmount" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">

                    <div class="col-md-2" style="padding-left: 5%;">
                        <input type="radio" class="form-check-input" id="radio3" name="gstradio" value="sgst" checked>
                        <label>SGST % : </label>
                    </div>
                    <div class="col-md-2">
                        <input type="text" id="txtSgst" placeholder="" name="txtSgst" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-1"><label>CGST % : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtCgst" placeholder="" name="txtCgst" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-1"><b>OR </b></div>

                    <div class="col-md-1">
                        <input type="radio" class="form-check-input" id="radio4" name="gstradio" value="igst">
                        <label>IGST % : </label>
                    </div>
                    <div class="col-md-2">
                        <input type="text" id="txtIgst" placeholder="" name="txtIgst" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                @*<div class="row form-group">
                <div class="col-md-3"><label>IGST % : </label></div>
                <div class="col-md-3">
                    <input type="text" id="txtIgst" placeholder="" name="txtIgst" class="form-control placeholder-no-fix" autocomplete="off">
                </div>
            </div>*@

                <div class="row form-group">
                    <div class="col-md-3"><label>Basic CESS : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtBasicCess" placeholder="" name="txtBasicCess" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Gross Amount : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtGrossAmount" placeholder="" name="txtGrossAmount" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><h5><b>Deduction Part : </b></h5></div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>IT TDS : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtItTds" placeholder="" name="txtItTds" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>SD Money : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtSdMoney" placeholder="" name="txtSdMoney" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>Gross CESS : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtGrossCess" placeholder="" name="txtGrossCess" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>TDS CGST : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtTdsCgst" placeholder="" name="txtTdsCgst" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>TDS SGST : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtTdsSgst" placeholder="" name="txtTdsSgst" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>P.F : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtPf" placeholder="" name="txtPf" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>P.F Advance : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtPfAdvance" placeholder="" name="txtPfAdvance" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>P. TAX : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtPtax" placeholder="" name="txtPtax" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>CCS Cont : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtCcsCount" placeholder="" name="txtCcsCount" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>CCS LIC : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtCcsLic" placeholder="" name="txtCcsLic" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>CCS Loan : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtCcsLoan" placeholder="" name="txtCcsLoan" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>CO-OP : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtCoop" placeholder="" name="txtCoop" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>G.I : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtGi" placeholder="" name="txtGi" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>LIC : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtLic" placeholder="" name="txtLic" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>

                    <div class="col-md-2"><label>Festival : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFestival" placeholder="" name="txtFestival" class="form-control placeholder-no-fix" autocomplete="off">
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Total Deducation : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtTotalDeduction" placeholder="" name="txtTotalDeduction" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Net Amount of Bill : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtNetAmountBill" placeholder="" name="txtNetAmountBill" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2">
                        <label>Select Payment Type :</label>
                    </div>
                    <div class="col-md-6">
                        <div class="form-check form-check-inline">
                            <input type="radio" class="form-check-input" id="radio1" name="optradio" value="1">Bank
                            <label class="form-check-label" for="radio1"></label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input type="radio" class="form-check-input" id="radio2" name="optradio" value="2">Treasury
                            <label class="form-check-label" for="radio2"></label>
                        </div>
                    </div>
                </div>

                <div class="container" style="background-color: beige;" id="BankContainer">
                    <div class="row form-group">
                        <div class="col-md-3"><label>Bank Name : </label></div>
                        <div class="col-md-3">
                            <select id="ddlBank" name="ddlBank" class="form-control placeholder-no-fix selectpicker" data-show-subtext="true" data-live-search="true">
                                <option value="0"> -- Select -- </option>
                            </select>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Account No : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtBankAccNo" placeholder="" name="txtBankAccNo" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Scheme : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtFundScheme" placeholder="" name="txtFundScheme" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                            <input type="hidden" name="txtFundSchemeId" id="txtFundSchemeId" />
                        </div>
                    </div>
                </div>

                <div class="container" style="background-color: beige;" id="TreasuryContainer">
                    <div class="row form-group">
                        <div class="col-md-3"><label>Scheme Name : </label></div>
                        <div class="col-md-3">
                            <select id="ddlScheme" name="ddlScheme" class="form-control placeholder-no-fix selectpicker" data-show-subtext="true" data-live-search="true">
                                <option value="0"> -- Select -- </option>
                            </select>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Advice No : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtAdviceNo" placeholder="" name="txtAdviceNo" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Advice Date : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtAdviceDate" placeholder="" name="txtAdviceDate" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>
                </div>

                <div class="form-group row justify-content-center text-center">
                    <div class="col-sm-12">
                        <button type="button" class="btn btn-info" id="btn_final_submit">Submit</button>
                    </div>
                </div>
            </div>

        </form>

    </div>
</div>

<script src="~/assets/admin/js/accountant.js"></script>
