
@{
    ViewBag.Title = "Amount Calculation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <div class="container" style="background-color: white; color: black; padding-bottom: 20px;">
        <div class="row no-gutters">
            <div class="flex-column justify-content-center about-content" style="width: 100% !important;">
                <div class="section-title">
                    <h2>Amount Calculation</h2>
                </div>
            </div>
        </div>


        <form id="voucher" method="post" style="display:block;">
            <div class="container" style="background-color: beige;">
                <div class="row form-group">
                    <div class="col-md-3"><label>Basic Amount of Bill : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtVouterNo" placeholder="" name="txtVouterNo" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>CGST % : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtVouterDate" placeholder="" name="txtVouterDate" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>SGST % : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtComapanyCategory" placeholder="" name="txtComapanyCategory" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>IGST % : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtWorkType" placeholder="" name="txtWorkType" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Basic CESS : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtComapanyName" placeholder="" name="txtComapanyName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Gross Amount : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtDeptName" placeholder="" name="txtDeptName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><h5>Deduction Part : </h5></div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>IT TDS : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>SD Money : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>Gross CESS : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>TDS CGST : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>TDS SGST : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>P.F. : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>P.F. Advance : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>P. TAX : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>CCS Cont : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>CCS LIC : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>CCS Loan : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>CO-OP : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2"><label>G.I : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtProprietorName" placeholder="" name="txtProprietorName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>LIC : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>

                    <div class="col-md-2"><label>Festival : </label></div>
                    <div class="col-md-2">
                        <input type="text" id="txtFundName" placeholder="" name="txtFundName" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Total Deducation : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-3"><label>Net Amount of Bill : </label></div>
                    <div class="col-md-3">
                        <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                    </div>
                </div>

                <div class="row form-group">
                    <div class="col-md-2">
                        <label>Select Payment Type :</label>
                    </div>
                    <div class="col-md-6">
                        <div class="form-check form-check-inline">
                            <input type="radio" class="form-check-input" id="radio1" name="optradio" value="1">Bank
                            <label class="form-check-label" for="radio1"></label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input type="radio" class="form-check-input" id="radio2" name="optradio" value="2">Treasury
                            <label class="form-check-label" for="radio2"></label>
                        </div>
                    </div>
                </div>

                <div class="container" style="background-color: beige;" id="BankContainer">
                    <div class="row form-group">
                        <div class="col-md-3"><label>Bank Name : </label></div>
                        <div class="col-md-3">
                            <select id="ddlFund" name="ddlFund" class="form-control placeholder-no-fix selectpicker" data-show-subtext="true" data-live-search="true">
                            </select>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Account No : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Scheme : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>
                </div>

                <div class="container" style="background-color: beige;" id="TreasuryContainer">
                    <div class="row form-group">
                        <div class="col-md-3"><label>Scheme Name : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Advice No : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-3"><label>Advice Date : </label></div>
                        <div class="col-md-3">
                            <input type="text" id="txtMobile" placeholder="" name="txtMobile" class="form-control placeholder-no-fix" autocomplete="off" disabled>
                        </div>
                    </div>
                </div>

                <div class="form-group row justify-content-center text-center">
                    <div class="col-sm-12">
                        <button type="button" class="btn btn-info" id="btn_approved">Procede for Payment</button>
                    </div>
                </div>
            </div>

        </form>

    </div>
</div>

